{
	"extends": "mrmlnc",
	"rules": {
		"no-magic-numbers": "off",
		"@typescript-eslint/no-magic-numbers": "off",
		"@typescript-eslint/prefer-string-starts-ends-with": "off"
	},
	"overrides": [
		{
			"files": [
				"**/*.e2e.ts"
			],
			"rules": {
				"unicorn/prevent-abbreviations": [
					"error",
					{
						"checkFilenames": false
					}
				]
			}
		}
	]
}
